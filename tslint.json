{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "no-empty": false,
    "semicolon": false,
    "ter-arrow-parens": false,
    "align": false,
    "space-before-function-paren": false,
    "import-name": false,
    "no-shadowed-variable": false
  }
}
